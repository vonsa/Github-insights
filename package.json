{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c --environment VERSION:$VERSION",
    "dev": "concurrently \"rollup -c -w\" \"npm run graphql:types:dev\"",
    "start": "sirv public --no-clear",
    "prepare": "husky install",
    "test:all": "npm run test:jest & npm run test:typescript & npm run test:prettier & npm run test:eslint:src & npm run test:svelte",
    "test:svelte": "svelte-check --fail-on-warnings --tsconfig ./tsconfig.json",
    "test:eslint": "eslint --max-warnings 0 --ext ts,tsx,js --no-ignore",
    "test:eslint:src": "npm run test:eslint -- ./src",
    "test:eslint:fix": "npm run lint -- --fix",
    "test:jest": "jest",
    "test:jest:watch": "npm test -- --watch",
    "test:typescript": "sh -c 'tsc -p tsconfig.json --noEmit'",
    "test:prettier": "prettier --check --loglevel=warn",
    "test:prettier:src": "npm run test:prettier -- src/**/*",
    "graphql:types": "npx apollo client:codegen src/GraphQL/types/ --localSchemaFile=schemas/github-api-schema.graphql --includes=src/GraphQL/Queries/** --target=typescript --tagName=gql --outputFlat",
    "graphql:types:dev": "npx apollo client:codegen src/GraphQL/types/ --localSchemaFile=schemas/github-api-schema.graphql --includes=src/GraphQL/Queries/** --target=typescript --tagName=gql --outputFlat --watch",
    "sentry:upload:sourcemaps": "sentry-cli releases files github-insights@$VERSION upload-sourcemaps ./public/build --url-prefix '~/build'",
    "sentry:release:finalize": "sentry-cli releases finalize github-insights@$VERSION"
  },
  "engines": {
    "node": "16.15.0"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "npm run test:eslint --",
      "npm run test:typescript --"
    ],
    "*.js": [
      "npm run test:eslint --"
    ],
    "*.svelte": [
      "npm run test:svelte --",
      "npm run test:prettier --"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.5",
    "@sentry/cli": "^2.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.0",
    "@types/animejs": "^3.1.4",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "jest-mock-axios": "^4.4.0",
    "lint-staged": "^11.1.2",
    "mock-apollo-client": "^1.2.0",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.56.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.4",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.38.1",
    "svelte": "^3.42.3",
    "svelte-check": "^2.2.5",
    "svelte-jester": "^1.8.2",
    "svelte-preprocess": "^4.8.0",
    "ts-jest": "^26.5.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@apollo/client": "^3.4.9",
    "@sentry/browser": "^7.2.0",
    "@sentry/tracing": "^7.2.0",
    "animejs": "^3.2.1",
    "axios": "^0.21.1",
    "bourbon": "^7.0.0",
    "concurrently": "^6.2.1",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.23.0",
    "glob": "^7.1.7",
    "graphql": "^15.5.1",
    "immer": "^9.0.5",
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "rxjs": "^7.3.0",
    "sirv-cli": "^1.0.14",
    "svelte-apollo": "^0.4.0",
    "svelte-spa-router": "^3.2.0",
    "xstate": "^4.23.1"
  }
}
